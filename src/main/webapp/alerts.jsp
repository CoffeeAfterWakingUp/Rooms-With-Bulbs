<%@ page contentType="text/html;charset=UTF-8" language="java" %>

<div class="mt-3">
    <div class="alert alert-success alert-dismissible fade show" role="alert" id="success" style="display: none;">
        <p>New Room created successfully!</p>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
    <div class="alert alert-info alert-dismissible fade show" role="alert" id="update" style="display: none;">
        <p>Room updated successfully!</p>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
    <div class="alert alert-danger alert-dismissible fade show" role="alert" id="error" style="display: none;">
        <p>Please fill out room name!</p>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
</div>

